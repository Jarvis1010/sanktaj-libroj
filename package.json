  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
